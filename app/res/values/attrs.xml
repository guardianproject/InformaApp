<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <attr format="string" name="font"/>
    
    <declare-styleable name="VerticalButton">
        <attr name="android:text" />
    </declare-styleable>
    
    <declare-styleable name="RoundedImageView">
        <attr name="rounding_radius" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="DropdownSpinner">
        <attr name="dropdown_background" format="reference" />
        <attr name="android:divider" />
    </declare-styleable>
    
    <declare-styleable name="CustomFontTextView">
        <attr name="font"/>
    </declare-styleable>
    
    <declare-styleable name="CustomFontEditText">
        <attr name="font"/>
    </declare-styleable>
    
</resources>